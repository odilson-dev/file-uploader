    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "ejs": "~2.6.1",
    "express": "~4.16.1",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.18.0",
    "express-validator": "^7.2.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "multer": "1.4.5-lts.1",
    "nodemon": "^3.1.4",
    "passport": "^0.7.0",
